<h1>Chat room</h1>



<%= form_for @message, remote: true do |f| %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>


<div class="msg_index">
  <ul id="msg_index">
    <% @messages.each do |msg| %>
      <li><%= msg.content %></li>
    <% end %>
  </ul>

</div>

<div id="msg">

</div>


